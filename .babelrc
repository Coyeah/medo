// .babelrc
{
	"presets": ["react","es2015","stage-0"],
  "env": {
    "development": {
    "plugins": [["react-transform", {
       "transforms": [{
         "transform": "react-transform-hmr",

         "imports": ["react"],

         "locals": ["module"]
       }]
     }]]
    }
  },
  "plugins": [
    "transform-runtime" // 用于运行generator
  ]
}
